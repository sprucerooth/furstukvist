{
  "multipart": [
    {
      "when" : { "type": "north_facing" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_slab" }
    },
    {
      "when" : { "type": "east_facing" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_slab", "y": 90 }
    },
    {
      "when" : { "type": "south_facing" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_slab", "y": 180 }
    },
    {
      "when" : { "type": "west_facing" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_slab", "y": 270 }
    },
    {
      "when" : { "type": "double", "axis": "x" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log", "x": 90 }
    },
    {
      "when" : { "type": "double", "axis": "z" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log", "x": 90, "y": 90 }
    }
  ]
}