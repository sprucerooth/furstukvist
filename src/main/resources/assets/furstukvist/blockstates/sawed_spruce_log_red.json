{
  "multipart": [
    {
      "when" : { "axis": "y", "facing": "north|south" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_red" }
    },
    {
      "when" : { "axis": "y", "facing": "east|west" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_red", "y": 90 }
    },
    {
      "when" : { "axis": "x" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_red", "x": 90, "y": 90 }
    },
    {
      "when" : { "axis": "z" },
      "apply": { "model": "furstukvist:block/sawed_spruce_log_red", "x": 90 }
    }
  ]
}